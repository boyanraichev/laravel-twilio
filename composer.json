{
    "name": "boyo/laravel-twilio",
    "description": "Laravel notifications with Twilio",
    "keywords": ["laravel", "notifications", "sms", "twilio"],
    "homepage": "https://github.com/boyanraichev/laravel-twilio",
    "license": "MIT",
    "authors": [
        {
            "name": "Boyan Raichev",
            "email": "myself@boyanr.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "laravel/framework": ">=8.0",
        "twilio/sdk": "^5.0"
    },
    "autoload": {
		"psr-4": {
			"Boyo\\Twilio\\": "src/"
    	}
	},
    "extra": {
	    "laravel": {
	        "providers": [
	            "Boyo\\Twilio\\TwilioServiceProvider"
	        ],
	        "aliases": {
		        "TwilioChannel": "Boyo\\Twilio\\TwilioChannel"
	        }
	    }
    }
}